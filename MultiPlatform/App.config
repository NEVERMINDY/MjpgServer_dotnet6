<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<clear/>
		<!--Port:						MjpgServer监听的端口-->
		<!--ReadJpegFrom:				设置MjpgServer应该从何处读取所需图片-->
		<!--GraphicProcessThreadNum:	设置多线程图像处理的最大线程数-->
		<!--RemoteServerIPEndPoint:		若MjpgServer从远端服务器读取突破，设置该值为远端服务器的 IP:Port-->
		<!--ImageFormat:				读取的图片的格式-->
		<!--请尽量将Port设置为2022，因为JavaScript中连接的是2022-->
		<add key="Port" value="2022"/>
		<!--允许的值:TCP/Memory-->
		<add key="ReadJpegFrom" value="Memory"/>
		<add key="ImagePath" value="D:\Program_Alan\MjpgServer_dotnet6\MultiPlatform\images"/>
		<add key="GraphicProcessThreadNum" value="2"/>
		<add key="RemoteServerIPEndPoint" value="10.132.60.231:1025"/>
		<add key="ImageFormat" value=".jpg"/>
	</appSettings>                           
</configuration>